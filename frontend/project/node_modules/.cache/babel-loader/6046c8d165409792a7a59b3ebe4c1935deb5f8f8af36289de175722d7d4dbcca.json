{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: 'LoginForm',\n  data() {\n    return {\n      userId: '',\n      password: ''\n    };\n  },\n  methods: {\n    goToMainPage() {\n      this.$router.push(\"/MainPage\");\n    },\n    goToSignUpPage() {\n      this.$router.push(\"/SignUpPage\");\n    },\n    checkInputs() {\n      if (!this.userId || !this.password) {\n        alert('아이디와 비밀번호를 모두 입력해주세요.');\n      } else {}\n    },\n    async loginUser() {\n      if (!this.userId || !this.password) {\n        alert('아이디와 비밀번호를 모두 입력해주세요.');\n        return;\n      }\n      try {\n        const response = await axios.post(\"http://localhost:3001/api/login\", {\n          studentID: this.userId,\n          password: this.password\n        });\n        if (response.status === 200) {\n          const {\n            message,\n            user,\n            token\n          } = response.data;\n          alert(\"로그인에 성공하였습니다!\");\n\n          // 로그인에 성공하면 사용자 정보와 토큰을 로컬 스토리지에 저장합니다.\n          localStorage.setItem('user', JSON.stringify(user));\n          localStorage.setItem('token', token);\n\n          // 로그인에 성공하면 Main 페이지로 이동합니다.\n          this.$router.push({\n            path: '/MainPage'\n          });\n        } else {\n          // 로그인이 실패한 경우, 백엔드에서 온 에러 메시지를 보여줍니다.\n          const {\n            error\n          } = response.data;\n          alert(error);\n        }\n      } catch (error) {\n        console.error(\"로그인 중에 문제가 발생했습니다. 다시 시도해주세요.\", error);\n        alert(\"학번 또는 비밀번호가 틀렸습니다. 다시 시도해주세요.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","userId","password","methods","goToMainPage","$router","push","goToSignUpPage","checkInputs","alert","loginUser","response","post","studentID","status","message","user","token","localStorage","setItem","JSON","stringify","path","error","console"],"sources":["/Users/moonjiin/Desktop/MatchingUs/frontend/project/src/views/LoginPage.vue"],"sourcesContent":["<template>\n\n<div class=\"LoginPageContainer\">\n    <form class=\"LoginPage\" @submit.prevent=\"goToMainPage\">\n    <div class=\"logo-container\">\n      <img src=\"/image/logo.png\" class=\"logo\" alt=\"매칭어스 로고\" />\n    </div>\n    <div class=\"int-area\">\n      <label for=\"id\"></label>\n      <input v-model=\"userId\" type=\"text\" name=\"id\" id=\"id\" placeholder=\"USER ID\" />\n      <label for=\"password\"></label>\n      <input v-model=\"password\" type=\"password\" name=\"pwd\" id=\"pwd\" placeholder=\"PASSWORD\" required />\n    </div>\n\n    <div class=\"btn-area\">\n        <button @click=\"loginUser\" type=\"submit\" id=\"login-button\" :disabled=\"!userId || !password\">\n          LOGIN\n        </button>\n      </div>\n\n    <div class=\"caption\">\n      <a href=\"#\" @click=\"goToSignUpPage\">회원가입</a>\n    </div>\n  </form>\n</div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: 'LoginForm',\n  data() {\n    return {\n      userId: '',\n      password: '',\n    };\n  },\n  methods: {\n    goToMainPage() {\n      this.$router.push(\"/MainPage\");\n    },\n    goToSignUpPage() {\n      this.$router.push(\"/SignUpPage\");\n    },\n    checkInputs() {\n      if (!this.userId || !this.password) {\n        alert('아이디와 비밀번호를 모두 입력해주세요.');\n      } else {\n      }\n    },\n    async loginUser() {\n      if (!this.userId || !this.password) {\n        alert('아이디와 비밀번호를 모두 입력해주세요.');\n        return;\n      }\n\n      try {\n        const response = await axios.post(\"http://localhost:3001/api/login\", {\n          studentID: this.userId,\n          password: this.password,\n        });\n\n        if (response.status === 200) {\n          const { message, user, token } = response.data;\n          alert(\"로그인에 성공하였습니다!\");\n          \n          // 로그인에 성공하면 사용자 정보와 토큰을 로컬 스토리지에 저장합니다.\n          localStorage.setItem('user', JSON.stringify(user));\n          localStorage.setItem('token', token);\n\n          // 로그인에 성공하면 Main 페이지로 이동합니다.\n          this.$router.push({\n            path: '/MainPage',\n          });\n        } else {\n          // 로그인이 실패한 경우, 백엔드에서 온 에러 메시지를 보여줍니다.\n          const { error } = response.data;\n          alert(error);\n        }\n      }  catch (error) {\n        console.error(\"로그인 중에 문제가 발생했습니다. 다시 시도해주세요.\", error);\n        alert(\"학번 또는 비밀번호가 틀렸습니다. 다시 시도해주세요.\");\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\nbody, html {\n  margin: 0;\n  padding: 0;\n  font-family: 'jua', sans-serif;\n}\n\n.LoginPageContainer {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  min-height: 100vh;\n  background: linear-gradient(to bottom right, white, #F6E6F6);\n  overflow: hidden; /* Prevent scrolling */\n  font-family: 'jua', sans-serif;\n}\n\n.LoginPage {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  position: fixed; /* Change to fixed */\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  width: 400px;\n  padding: 20px;\n  background-color: #fff;\n  border-radius: 50px;\n}\n\n.logo-container {\n  text-align: center;\n  margin-bottom: 20px;\n  width: 100%; /* Add this line */\n}\n\n.logo {\n  max-width: 70%;\n  height: auto;\n  display: inline-block; /* Add this line */\n}\n\n.LoginPage h1 {\n  font-size: 32px;\n  text-align: center;\n  margin-bottom: 30px;\n}\n\n.int-area input {\n  width: 100%;\n  padding: 15px;\n  border: 1px solid #ccc;\n  border-radius: 5px;\n  font-size: 16px;\n  margin-bottom: 15px;\n  font-family: 'jua', sans-serif;\n}\n\n\n.btn-area button {\n  width: 100%; /* Allow the button to expand to the parent width */\n  max-width: 300px; /* Set a maximum width for the button */\n  margin: 0 auto; /* Center the button horizontally */\n  padding: 15px; /* Adjust padding for spacing */\n  background: #007bff;\n  color: white;\n  font-size: 16px;\n  border: none;\n  border-radius: 20px;\n  cursor: pointer;